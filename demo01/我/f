//调用父类的构造方法。

package inheritanceAndPolymorphism;

public class demo7 {
	class Employee{
		String name;
		public Employee(String s){		//父类的构造方法。
			name = s;
		}
	}
	class Manager extends Employee{
		String deparment;
		public Manager(String s,String d) {		//子类的构造方法。
			super(s);		//调用父类的构造方法。
			deparment = d;
		}
	}
}
